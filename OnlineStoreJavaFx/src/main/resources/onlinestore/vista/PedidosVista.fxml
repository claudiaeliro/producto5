<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="onlinestore.controlador.PedidosController">
   <children>
      <Pane layoutX="-1.0" prefHeight="400.0" prefWidth="600.0" />
      <Pane layoutX="9.0" layoutY="6.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="204.0" layoutY="2.0" prefHeight="96.0" prefWidth="206.0" text="Pedidos" textAlignment="CENTER">
               <font>
                  <Font name="Arial" size="46.0" />
               </font>
            </Label>
            <Button fx:id="mostrarEnviados" layoutX="133.0" layoutY="269.0" mnemonicParsing="false" onAction="#botonEnviados" prefHeight="32.0" prefWidth="340.0" text="Mostrar pedidos enviados">
               <font>
                  <Font size="21.0" />
               </font>
            </Button>
         </children>
      </Pane>
      <Button fx:id="añadirPedido" layoutX="142.0" layoutY="110.0" mnemonicParsing="false" onAction="#botonAñadirPedido" prefHeight="32.0" prefWidth="340.0" text="Añadir Pedido">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="eliminarPedido" layoutX="142.0" layoutY="165.0" mnemonicParsing="false" onAction="#botonEliminarPedido" prefHeight="32.0" prefWidth="340.0" text="Eliminar Pedido">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="mostrarPendientesEnvio" layoutX="142.0" layoutY="220.0" mnemonicParsing="false" onAction="#botonPendientesEnvio" prefHeight="32.0" prefWidth="340.0" text="Mostrar pendientes envio">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Button fx:id="volverPedidos" layoutX="142.0" layoutY="330.0" mnemonicParsing="false" onAction="#botonVolverPedidos" prefHeight="32.0" prefWidth="340.0" text="Volver">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
